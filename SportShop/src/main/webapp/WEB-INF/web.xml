  </servlet>
  <servlet-mapping>
    <servlet-name>ProductControllers</servlet-name>
    <url-pattern>/ProductControllers</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>ShopServlet</display-name>
